{
  "createUrl": "https://api.exemplo.com/conversations",
  "messageUrl": "https://api.exemplo.com/messages",
  "headers": {
    "Authorization": "Bearer stress-test-token",
    "Content-Type": "application/json"
  },
  "createBody": "{\"title\":\"Stress Test\",\"priority\":\"high\"}",
  "messageBodyTemplate": "{\"conversationId\":\"{{conversationId}}\",\"content\":\"Mensagem de stress test\",\"metadata\":{\"test\":\"load\",\"timestamp\":\"{{timestamp}}\"}}",
  "concurrency": 50,
  "durationSec": 300,
  "messagesPerConversation": 10,
  "timeoutMs": 5000,
  "output": "stress-test-results.json"
}
